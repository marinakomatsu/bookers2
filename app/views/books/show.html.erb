<%= render 'books/list', book: @book, user: @user %>

<h1>Book detail</h1>
  <tbody>
    <tr>
      <td><%= image_tag @user.get_profile_image(100,100) %></td>
      <br>
        <td><%= link_to @book.user.name, books_path(@book.id) %></td>
        <th><%= link_to @book.body, book_path(@book.id) %></th>
        <th><%= @book.body %></th>
    </tr>   
  </tbody>