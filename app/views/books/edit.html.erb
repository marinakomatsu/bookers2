<h1>User info</h1>
<%= form_with model: @book do |f| %>
  <h4>Name</h4>
  <%= f.text_field :shop_name %>
  <h4>Image</h4>
  <%= form_with model: @book do |f| %>
  
  <h4>Image</h4>
  <%= f.file_field :image, accept: "image/*" %>
  
  <h4>Introduction</h4>
  <%= f.text_area :caption %>
  <%= f.submit 'Update User' %>
<% end %>
